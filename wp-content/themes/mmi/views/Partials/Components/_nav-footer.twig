<nav id="footer_nav-footer" class="flex" role="navigation">

  <ul class="list-inline">
  {% for item in menu.footer.get_items %}
    <li class="nav-item footer-level {{item.classes | join(' ')}}">
      <a class="nav-link" href="{{item.get_link}}">{{item.title}}</a>
      {% if item.get_children %}
        <ul class="nav nav-drop">
        {% for child in item.get_children %}
          <li class="nav-drop-item {{child.classes | join(' ')}}">
            <a class="nav-link" href="{{child.get_link}}">{{child.title}}</a>
          </li>
        {% endfor %}
        </ul>
      {% endif %}
    </li>
  {% endfor %}
  </ul>

  <div class="nav-footer_reseaux-sociaux flex">
    <a href=""><img src="{{theme.path}}/assets/img/linkedin-font-awesome.svg" alt="Linkedin"></a>
    <a href=""><img src="{{theme.path}}/assets/img/facebook-font-awesome.svg" alt="Facebook"></a>
    <a href=""><img src="{{theme.path}}/assets/img/twitter-font-awesome.svg" alt="Twitter"></a>
  </div>
</nav>
